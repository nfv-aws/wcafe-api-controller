{
  "family": "wcafe",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "wcafe-app",
      "image": "wcafe-app",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": 
       {
        "logDriver": "awslogs", 
        "options": {
          "awslogs-region": "us-west-2", 
          "awslogs-stream-prefix": "fargate-task-1", 
          "awslogs-group": "ecs-log-streaming"
       }
      } 
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}
